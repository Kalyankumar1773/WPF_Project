<Window x:Class="xCubeApplication.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:xCubeApplication"
        xmlns:local1="clr-namespace:xCubeApplication.Views.Dashboard"
        xmlns:local2="clr-namespace:xCubeApplication.ViewModels.Dashboard"
        mc:Ignorable="d"
        WindowState="Maximized"
        WindowStartupLocation="CenterScreen"
        ResizeMode="CanResize"
        WindowStyle="SingleBorderWindow"
        Icon="pack://application:,,,/xCubeApplication;component/Images/app.png"
        Title="[x] Cube LABS" Height="450" Width="800">
    <Window.DataContext>
        <local2:DashboardViewModel/>
    </Window.DataContext>
    <Grid>
        <local1:DashboardView/>
    </Grid>
</Window>
